<h1>質問</h1>

<%= form_with model: @question, local: true do |f| %>

<div class="field">
  <%= f.label :video %>
  <%= f.file_field :video_question, :accept => 'video/*' %>
  <%= f.label :content %>
  <%= f.text_area :content%>
</div>

<div class="#">
  <%= f.submit "投稿する" ,class:"#" %>
  <%=link_to 'もどる', root_path, class:"back-btn" %>
</div>
<% end %>